{
  "name": "variancebeyondi-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch app --exec node app/server.js --inspect=9222",
    "start": "yarn run up && node app/server.js",
    "up": "knex migrate:latest",
    "down": "knex-migrate down",
    "rollback": "knex-migrate rollback",
    "down-all": "knex-migrate down --to 0",
    "seed": "knex seed:run",
    "migration": "knex migrate:make",
    "remigrate": "npm run down-all && npm run up",
    "test": "cross-env NODE_ENV=test yarn up && cross-env NODE_ENV=test mocha ./tests/api/**/*.test.js ./tests/admin/**/*.test.js || exit 0",
    "test-pre-commit": "cross-env NODE_ENV=test yarn up && cross-env NODE_ENV=test mocha ./tests/api/**/*.test.js ./tests/admin/**/*.test.js",
    "eslint": "./node_modules/.bin/eslint --ext .js --color ./ || exit 0",
    "eslint-pre-commit": "./node_modules/.bin/eslint --ext .js --color ./"
  },
  "pre-commit": [
    "test-pre-commit",
    "eslint-pre-commit"
  ],
  "dependencies": {
    "babel-eslint": "^8.2.6",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.3",
    "currently-unhandled": "^0.4.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "jsonwebtoken": "^8.1.0",
    "knex": "^0.13.0",
    "knex-migrate": "^1.3.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "morgan": "^1.8.1",
    "pg-promise": "^5.6.4",
    "solc": "^0.4.23",
    "squel": "^5.11.1",
    "web3": "^0.19.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cross-env": "^4.0.0",
    "csvtojson": "^1.1.7",
    "db-migrate": "^0.10.0-beta.24",
    "db-migrate-pg": "^0.1.11",
    "eslint": "^4.9.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "uuid": "^3.2.1"
  },
  "author": "",
  "license": "ISC"
}
